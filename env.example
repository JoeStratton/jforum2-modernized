# HAI Agent Framework Configuration
# Copy this file to .env and fill in your actual values

# Langfuse Configuration for Tracing
LANGFUSE_SECRET_KEY=your_secret_key_here
LANGFUSE_PUBLIC_KEY=your_public_key_here
LANGFUSE_BASE_URL=https://langfuse.hai.app.presidio.com
LANGFUSE_USER_ID=your_user_id_here

# AWS Bedrock Configuration (if needed for specific models)
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key

# Optional: Set logging level
LOG_LEVEL=info 

# Debug Configuration (optional)
# HAI_DEBUG=true  # Uncomment to enable debug logging